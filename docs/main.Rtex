        frecuencia_plot_combined_7YTD <- dataCuestionario %>%
          mutate(plot_combined_7YTD = ifelse(plot_combined_7YTD == 'p', 'SQM', ifelse(plot_combined_7YTD == 'q', 'ITUB4', plot_combined_7YTD))) %>%
          group_by(plot_combined_7YTD) %>%
          summarise(frecuencia = n())

        frecuencia_plot_combined_7YTD$plot_combined_7YTD <- factor(frecuencia_plot_combined_7YTD$plot_combined_7YTD, levels = c('SQM','ITUB4'))
        
        # Crea el gráfico de barras
        plot_combined_7YTD_response <- ggplot(frecuencia_plot_combined_7YTD, aes(x = plot_combined_7YTD, y = frecuencia, fill = plot_combined_7YTD)) +
          geom_bar(stat = "identity") +
          labs(
               x = "Respuestas",
               y = "Frecuencia") +
          theme_minimal() +
          scale_fill_manual(values = c("#468189", "#77ACA2")) +
          theme(legend.position = "none")
          print(plot_combined_7YTD_response)
          @
    \caption{Respuestas para gráfico a 7 años (S\&P y Bloomberg)}
